Subject: [PATCH] android-nn-driver: link libarmnnSerializer

---
 Makefile | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Makefile b/Makefile
index d1d4385..e020d0e 100644
--- a/Makefile
+++ b/Makefile
@@ -38,7 +38,7 @@ CFLAGS = -Wall -Werror -Wextra -std=c++17 -fPIC -fvisibility=default -Wno-incons
 
 CXXFLAGS += $(DEFINES) $(INCLUDES) $(CFLAGS)
 
-LIBS = -larmnn
+LIBS = -larmnn -larmnnSerializer
 
 $(LIBNAME): $(OBJECTS)
 	$(CC) -shared -Wl,-soname,$(SONAME) -o $@ $^ -lpthread -lrt -ltensorflowlite ${LIBS}
